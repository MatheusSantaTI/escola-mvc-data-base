// controllers/curso.controller.js

function listar(req, res) {
  res.send("Listando cursos...");
}

function criar(req, res) {
  res.send("Criando curso...");
}

function atualizar(req, res) {
  res.send("Atualizando curso...");
}

function deletar(req, res) {
  res.send("Deletando curso...");
}

module.exports = {
  listar,
  criar,
  atualizar,
  deletar
};
